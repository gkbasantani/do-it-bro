{
  "expo": {
    "name": "Do It Bro",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/do-it-bro-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/do-it-bro-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/do-it-bro-adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.doitbro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
